RUN apt install --yes \
    build-essential \
    autotools-dev \
    automake \
    cscope 

ENV TZ=Europe/London
RUN ln -snf /usr/share/zoneinfo/$TZ /etc/localtime && echo $TZ > /etc/timezone
RUN apt install -y pkg-config
    
